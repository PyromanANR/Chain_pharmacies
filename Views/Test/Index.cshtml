@model IEnumerable<Product>

    <h1>Test</h1>
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    1
                </th>
                <th>
                    2
                </th>
                <th>
                    3
                </th>
                <th>
                    4
                </th>
                <th>
                    5
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
            <tr>
                <td>
                    @obj.Id
                </td>
                <td>
                    @obj.BrandId
                </td>
                <td>
                    @obj.TypeId
                </td>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.Description
                </td>
            </tr>
        }
        </tbody>
    </table>
